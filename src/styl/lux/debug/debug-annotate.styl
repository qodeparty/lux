#annotate 
  padding 10px
  
  #inline div 
    color:#bbb
    
  #inline div *:first-child 
    color:#000

  .break 
    border:none; border-top:1px dotted #eee; padding-top:10px
  
  .grouping 
    border:1px dotted #75B8FB; background:#eee; padding:30px; margin:50px
    &.note 
        color:#FA6563; display:block; margin-bottom:5px; font-weight:bold
        
  .psuedo
    border:1px dashed #FFA64D; height:20px; position:relative
    &:before
      content: attr(title);
      top:20%;
      left:50%;
      position:absolute;
      color: #FFA64D;
      font-family:sans-serif;
      font-size:9px
  

  .ref   
    border:none; border:1px dashed #FFA64D; padding:1px 3px; 
    font-family:sans-serif; color:#FFA64D; font-size:10px; text-transform:uppercase; line-height:1.5pt 
    
  .notes,.note 
    font-family:sans-serif; color:#eee; font-size:9px;
  

  .title 
    font-family:sans-serif; color:#eee; font-size:9px; text-transform:uppercase
    
  .code  
    display:none
    
  [data-tag] 
    border:1px dotted #75B8FB; margin:5px 0; padding:5px;
    &:after
          content: "";
          display: table;
          clear: both;
          
  .clearfix
    display:block
    &:after
      clear:both;content:".";display:block;font-size:0;height:0;line-height:0;visibility:hidden


    
  &.bg-grid
      background-image:
        linear-gradient(rgba(255,255,255,.1) 0px, transparent 1px),
        linear-gradient(90deg, rgba(255,255,255,.1) 0px, transparent 1px),
        linear-gradient(rgba(255,255,255,.1) 1px, transparent 1px),
        linear-gradient(90deg, rgba(255,255,255,.1) 1px, transparent 1px);
      background-size:100px 100px, 100px 100px, 20px 20px, 20px 20px;
      background-position:-1px -1px, -1px -1px, -1px -1px, -1px -1px;


